# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'Forest Guardian' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Forest Guardian
  pod 'Mapbox-iOS-SDK', '~> 3.2.3'
  pod 'MapboxGeocoder.swift', :git => 'https://github.com/mapbox/MapboxGeocoder.swift.git', :tag => 'v0.5.0'
  pod 'AFNetworking', '~> 3.0'
  pod "HockeySDK"

  target 'Forest GuardianTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Forest GuardianUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
